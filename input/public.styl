@import "reset.css";
/*@import "function.css";*/
/*

-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=1,startColorstr=#99ffffff,endColorstr=#99ffffff)";  
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#99ffffff,endColorstr=#99ffffff);
background:rgba(255,255,255,.8);


filter:alpha(opacity=40); 针对 IE8 以及更早的版本 */

.w
	max-width 1032px
	width 100%
	margin 0 auto
body{
	font-size: 100%;
	font-family:'fzltzh','Arial','microsoft yahei';
}
@font-face {
	font-family: 'fzltzh';  /*给自定义字体命名*/
	src: url('../font/_gbk.woff') format('woff'),
	url('../font/_gbk.ttf') format('truetype'),
	url('../font/_gbk.svg#webfontjKg17VrE') format('svg');
}

.header 
	height 210px
	background url('../img/hico2.png') center 18px no-repeat ,
	url('../img/hico1.png') center top /1600px 100% no-repeat
	.logo
	.sub-logo
		display inline-block
		margin-top 49px
		height 110px
	.logo
		width 340px
		background url('../img/hico3.png') center center/100% auto no-repeat
	.sub-logo
		width 378px
		margin-left 20px
		background url('../img/hico4.png') center center/100% auto no-repeat
	.nav
		padding-top 2px
		box-sizing border-box
		height 36px
		font-size 14px
		color #473728
		line-height 36px
		text-align center
		background url('../img/hico5.png') center top no-repeat
		span
			display inline-block
			height 36px
			padding 0 34px
			+span
				background url('../img/hico7.png') left center no-repeat

.main
	background url('../img/hico9.png') center top no-repeat
	.banner	
		.swiper-button-next,
		.swiper-button-prev
			width 47px
			height 70px
			margin-top -(@height/2)px
			background-size 100% 100%
			background-position 0 0
		.swiper-button-next
			right 0
			background-image url(../img/hico27.png)
		.swiper-button-prev
			left 0
			background-image url(../img/hico26.png)
		.swiper-container
			a
			img
				display block
				width 100%
	.section
		background url('../img/hico17.png') center 8px no-repeat,
		url('../img/hico18.png') center bottom 8px no-repeat,
		url('../img/hico25.png') center 0 no-repeat,
		url('../img/hico1.png') left top repeat
		h2
			width 100%
			padding 70px 45px 62px
			margin-left -45px
			font-size 22px
			color #ffffd1
			text-transform uppercase
			letter-spacing 3px
			text-align center
			line-height 50px
			background url('../img/hico19.png') center 70px no-repeat
		.tips
			margin-top 15px
			padding-top 22px
			padding-right 40px
			height 102px
			text-align center
			font-size 18px
			color #434135
			line-height 1.5
			background url(../img/hico24.png) center top /100%  auto no-repeat
		.content
			padding 0 30px
			overflow hidden
			background url('../img/hico21.png') center top no-repeat
			&.childer-show
				padding-bottom 30px
				background url(../img/hico47.png) center top no-repeat
				&.childer-show-2
					.right
						p
							font-size 16px
							line-height 1.8
							background-image none
							padding-bottom 0
							&:first-child
								padding-top 12px
								background-image url(../img/hico53.png)
								background-position left top
				&.childer-show-3
					.right
						p.fz24
							font-size 24px
							color #343325
							background-image url(../img/hico53.png)
							background-position left bottom
						p
							&:last-child
								padding-bottom 12px
								background-image url(../img/hico53.png)
								background-position left bottom		
							img
								margin-top 20px
								margin-bottom 20px
				&.childer-show-4
					.right
						p.fz24
							padding-bottom 10px
							font-size 16px		
						p.pt10
							padding-top 10px
				&.about
					.about-c
						padding-top 56px
						p
							text-align center
				&.style2
					margin-top 30px
					padding-top 20px
					padding-bottom 20px
					.left
						float right
						img
							padding-top 50px
					.right
						float left
						p
							font-size 14px
							line-height 1.4
							background-image none
							padding-bottom 0
							&:last-child
								padding-bottom 12px
								background-image url(../img/hico53.png)
						
				h3
					padding-bottom 10px
			h3
				margin 18px 0 0 16px
				padding-left 25px
				line-height 40px
				font-size 24px
				color #5e5b48
				font-weight bold
				height 56px
				background url(../img/hico28.png) left top no-repeat
			.row
				padding 0 40px 0 30px
				img
					display block
				.left
					float left
				.right
					float right
					width 640px
					p
						padding-bottom 12px
						font-size 18px
						color #605c4b
						line-height 2
						background url(../img/hico23.png) left bottom no-repeat
						&.bgn
							padding 12px 0
							background none
				.new-line
					padding-bottom 100px
					.left
						width 620px
					.right
						width auto
					p
						background none
						img
							display block
							width 100%
		.quick-browse
			padding 44px 30px 80px
			h3
				height 46px
				font-size 24px
				color #433426
				line-height 46px
				background url(../img/hico38.png) right center no-repeat
			.wrap
				padding-left 20px
				height 170px
				background url(../img/hico29.png) 0 0 no-repeat
				h4
					font-size 18px
					color #c3af82
					line-height 40px
					text-transform uppercase
					background url(../img/hico39.png) left bottom no-repeat
				.swiper-container
					padding 10px 0
					height 115px
					box-sizing border-box
					margin-left 44px
					width 790px
					a
					img
						display block
						margin 0 auto
				.swiper-button-next,
				.swiper-button-prev
					width 15px
					height 35px
					margin-top -(@height/2)px
					background-size auto auto
					background-position 0 0
				.swiper-button-next
					right 80px
					background-image url(../img/hico37.png)
				.swiper-button-prev
					left 10px
					background-image url(../img/hico36.png)
		.childer-1-bottom
			padding 0 30px 70px
			.left
				float left
				width 640px
				.tips
					margin-top 70px
			.right
				float right
				width 240px
				img
					display block
					width 100%
		.location
			position relative
			margin 0 50px
			.right
			.left
				width 430px
				font-size 18px
				color #605c4b
				line-height 1.8
			.left
				float left
			.right
				float right
				img
					position static
					visibility hidden
			img
				position absolute
				bottom 0
				right 0
		.childer-show-2-content
			padding 0 0 35px
			&.childer-show-3-content
				.childer-show-2-body
					position relative
					background url(../img/hico55.png) center top no-repeat
					.left
						img
							padding-left 0
							width 474px
					.right
						padding-top 30px
						width 457px
					.w100
						padding-top 30px
						padding-bottom 50px
						float left
						width 100%
						font-size: 16px;
						color: #7d7a6b;
						line-height: 2;
					.tips
						position absolute
						bottom -20px
						right 30px
						width 500px
						font-size 16px
						padding-top 20px
						padding-right 30px
			&.childer-show-4-content
				.childer-show-4-body
					.left
						padding-left 15px
						img
							width 234px
					.right
						width 700px
						p
							font-size 18px
							color #7d7a6b
							line-height 1.8
					.tips-wrap
						float left
						width 100%
						box-sizing border-box
						padding 0 20px 0 30px
					.tips
						padding-top 33px
						width 100%
						box-sizing border-box
						float left
						height 155px
						position static
						font-size 28px
			&.childer-show-5-content
				.childer-show-5-body
					padding-bottom 20px
					.tips
						font-size 16px
						height 112px
					.p10-0
						padding 10px 0
						img
							padding-top 0
			&.about-content
				.about-body
					.tips
						margin-top -24px
						width  686px
						float right
			h3
				padding 20px 30px
				font-size 20px
				color #807d6d
				line-height 1.5
				font-weight 100
				letter-spacing -1px
				text-transform uppercase
			.childer-show-2-body
				padding 25px 30px 30px
				background url(../img/hico55.png) center top no-repeat,
				url(../img/hico58.png) -70px bottom no-repeat
				.right
					width 640px
					p
						font-size 16px
						color #7d7a6b
						line-height 2
						&:last-child
							padding-bottom 10px
					img{
						display block
						padding-top 20px
						width 100%
					}
				.left
					padding-top 30px
					img
						padding-left 30px
.footer
	padding-top 20px
	height 194px
	box-sizing border-box
	background url(../img/hico40.png) center top no-repeat
	a
		display block
		img
			display block
			margin 0 auto
	p
		padding-top 10px
		text-align center
		font-size 16px
		color #dedcd0
		line-height 1.5
